<%- model_class = Upload -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:live_id) %>:</strong></dt>
  <dd><%= @upload.live_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:src) %>:</strong></dt>
  <dd><%= @upload.src %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dst) %>:</strong></dt>
  <dd><%= @upload.dst %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @upload.status %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              uploads_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_upload_path(@upload), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              upload_path(@upload),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
