<%- model_class = LiveProgram -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<div>
  <%= paginate(@live_programs) %>
</div>
<div>
  <%= page_entries_info(@live_programs) %>
</div>
<%= form_tag({action: :search}, {method: :get, class: 'well form-search'}) do %>
  <%= text_field_tag :q, "#{params[:q]}", class: 'input-medium search-query' %>
  <%= submit_tag 'search', class: 'btn' %>
<% end %>
<table class="table table-striped">
  <thead>
    <tr>
      <!-- <th><%= model_class.human_attribute_name(:id) %></th> -->
      <th><%= model_class.human_attribute_name(:live_id) %></th>
      <th><%= model_class.human_attribute_name(:started_at) %></th>
      <!-- <th><%= model_class.human_attribute_name(:user) %></th> -->
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:desc) %></th>
      <!-- <th><%= model_class.human_attribute_name(:url) %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:player_status) %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:dl_status) %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:flv) %></th> -->
      <th><%= model_class.human_attribute_name(:mp4) %></th>
      <!-- <th><%= model_class.human_attribute_name(:created_at) %></th> -->
      <!-- <th><%=t '.actions', :default => t("helpers.actions") %></th> -->
    </tr>
  </thead>
  <tbody>
    <% @live_programs.each do |live_program| %>
      <tr>
        <!-- <td><%= link_to live_program.id, live_program_path(live_program) %></td> -->
        <td><%= link_to live_program.live_id, live_program_path(live_program) %></td>
        <td><%= live_program.started_at %></td>
        <!-- <td><%= live_program.user %></td> -->
        <td><%= live_program.title %></td>
        <td><%= live_program.desc %></td>
        <!-- <td><%= live_program.url %></td> -->
        <!-- <td><%= live_program.player_status %></td> -->
        <!-- <td><%= live_program.dl_status %></td> -->
        <!-- <td><%= link_to 'DL', live_program.flv_url if live_program.flv %></td> -->
        <td><%= link_to 'DL', live_program.mp4_url if live_program.mp4 %></td>
        <!-- <td><%=l live_program.created_at %></td> -->
        <!-- <td>
        <%= link_to t('.edit', :default => t("helpers.links.edit")),
        edit_live_program_path(live_program), :class => 'btn btn-default btn-xs' %>
        <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
        live_program_path(live_program),
        :method => :delete,
        :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
        :class => 'btn btn-xs btn-danger' %>
        </td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<div>
  <%= paginate(@live_programs) %>
</div>

